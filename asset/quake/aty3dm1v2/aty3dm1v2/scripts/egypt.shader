		map models\mapobjects\egypt\blackdog.tga
		rgbGen identityLighting
	}
	{
		map models\mapobjects\egypt\enviromap-gold.tga
		blendfunc filter
		rgbGen identityLighting
		tcGen environment 
	}

	{
		map $lightmap 
		blendfunc filter
		rgbGen identity
	}
}

