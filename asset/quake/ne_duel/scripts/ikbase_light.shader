
textures/ikbase_light/ik_gtrimh_lit1a_1000
{
	q3map_lightimage textures/ikbase_light/ik_gtrimh_lit1a_glow.tga
	qer_editorimage textures/ikbase_light/ik_gtrimh_lit1a.tga
	surfaceparm nomarks
	q3map_surfacelight 1000

	{
		map $lightmap
		rgbGen identity
	}

	{
		map textures/ikbase_light/ik_gtrimh_lit1a.tga
		blendFunc blend
	}
        
	{
		map textures/ikbase_light/ik_gtrimh_lit1a_glow.tga
		blendFunc add
		rgbGen wave noise .3 .2 0 10
	}
	
}

