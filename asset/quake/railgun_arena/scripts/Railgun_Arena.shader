textures/bata3dm1/bata3dm1_floor6
{
	qer_editorimage textures/bata3dm1/bata3dm1_jp.tga
	{
		map textures/bata3dm1/bata3dm1_jp2.tga
		tcMod rotate 180
		tcMod stretch sin 0.8 0.1 0 0.5 
	}
	{
		clampmap textures/bata3dm1/bata3dm1_jp1.tga
		blendfunc add
		rgbGen identity
		tcMod stretch sin 2.5 1 0 1.5 
	}
	{
		map textures/bata3dm1/bata3dm1_jp.tga
		blendfunc blend
		rgbGen identity
	}
	{
		map $lightmap 
		blendfunc gl_dst_color gl_one_minus_dst_alpha
		rgbGen identity
		tcGen lightmap 
	}
}
